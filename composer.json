	"type": "contao-module",
	"homepage":"https://github.com/respinar/prayertime/",
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Hamid Abbaszadeh",
			"email": "info@respinar.com",
			"homepage": "http://respinar.com/",

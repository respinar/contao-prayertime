{
    "name": "respinar/contao-praytimes",
    "description": "Pray times Extension for Contao",
    "keywords": ["contao", "pray times","prayer times"],
    "type": "contao-module",
    "homepage": "https://respinar.com",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Hamid Abbaszadeh",
            "email": "info@respinar.com",
            "homepage": "https://respinar.com",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/respinar/contao-praytimes/issues"
    },
    "require": {
        "php":">=5.4",
        "contao/core-bundle":"~3.5 || ~4.3",
        "contao-community-alliance/composer-plugin":"^2.4.1 || ~3.0"
    },
    "replace": {
        "contao-legacy/praytimes": "*"
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/praytimes"
            },
            "transifex": {
                "project": "contao-praytimes",
                "prefix": "master-",
                "languages_cto": "languages",
                "languages_tx": ".tx"
            }
        }
    }
}

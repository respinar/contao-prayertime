{
    "name": "respinar/contao-prayertime",
    "description": "Prayer times Module for Contao",
    "keywords": ["contao", "prayertime"],
    "type": "contao-module",
    "homepage": "https://github.com/respinar/contao-prayertime",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Hamid Abbaszadeh",
            "email": "info@respinar.com",
            "homepage": "http://respinar.com",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/respinar/contao-prayertime/issues"
    },
    "require": {
        "php": ">=5.3.2",
        "contao/core": ">=3.2,<4-dev",
        "contao-community-alliance/composer-plugin": "~2.0"
    },
    "replace": {
        "contao-legacy/prayertime": "*"
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/prayertime"
            },
            "transifex": {
                "project": "contao-prayertime",
                "prefix": "master-",
                "languages_cto": "languages",
                "languages_tx": ".tx"
            }
        }
    }
}
